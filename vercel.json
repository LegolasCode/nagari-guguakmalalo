{
    "version": 2,
    "framework": null,
    "functions": {
      "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
      { "src": "/(.*\\.[A-Za-z]{2,4})$", "dest": "/public/$1" },
      { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "outputDirectory": "public",
    "build": {
      "env": {
        "NODE_OPTIONS": "--max-old-space-size=4096"
      }
    },
    "installCommand": "npm install && php artisan storage:link",
    "buildCommand": "npm run build && php artisan migrate --force",
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "false",
